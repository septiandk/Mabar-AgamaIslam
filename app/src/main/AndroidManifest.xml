<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.book.dk.PAI">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconutama"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.book.dk.PAI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.book.dk.PAI.HomeActivity">

            <!-- Default Intent Filter -->
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.book.dk.PAI.Menulis" />
        <activity android:name="com.book.dk.PAI.Membaca" />
        <activity android:name="com.book.dk.PAI.Menu" />
        <activity android:name="com.book.dk.PAI.ListFragment" />
        <activity android:name="com.book.dk.PAI.Pdf_sampleFragment" />
        <activity android:name="com.book.dk.PAI.community" />
        <activity android:name="com.book.dk.PAI.store"/>
    </application>

</manifest>